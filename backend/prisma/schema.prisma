generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          String   @id @default(cuid())
  name        String
  lastName    String
  email       String   @unique
  password    String?
  provider    String?
  googleId    String?  @unique
  image       String?
  phoneNumber String?  @unique
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("users")
}
